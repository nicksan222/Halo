{
  "name": "@acme/ui",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "main": "src/index.ts",
  "types": "src/index.ts",
  "files": [
    "src"
  ],
  "exports": {
    ".": {
      "import": "./src/index.ts",
      "types": "./src/index.ts"
    },
    "./components/*": {
      "import": "./src/components/*.tsx",
      "types": "./src/components/*.tsx"
    },
    "./styles": {
      "import": "./src/styles/globals.css",
      "types": "./src/styles/globals.css"
    },
    "./lib/*": {
      "import": "./src/lib/*.ts",
      "types": "./src/lib/*.ts"
    },
    "./hooks/*": {
      "import": "./src/hooks/*.ts",
      "types": "./src/hooks/*.ts"
    }
  },
  "scripts": {
    "build": "bunx tsc -p tsconfig.json --noEmit",
    "typecheck": "bunx tsc -p tsconfig.json --noEmit",
    "lint": "bunx biome check .",
    "format": "bunx biome check --write .",
    "check": "bun run typecheck && bun run lint",
    "dev": "storybook dev -p 6006 --no-open",
    "build-storybook": "storybook build"
  },
